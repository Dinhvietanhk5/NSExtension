<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NSEdittext">
        <attr name="android:fontFamily" />
        <attr name="android:textColor" />
        <attr name="android:hint" />
        <attr name="android:text" />
        <attr name="android:textColorHint" />
        <attr name="android:imeOptions" />
        <attr name="android:textStyle" />
        <attr name="android:textAllCaps" />
        <attr name="android:gravity" />
        <attr name="android:textSize" />
        <attr name="android:background" />
        <attr name="android:drawableStart" />
        <attr name="android:drawableEnd" />
        <attr name="android:drawablePadding" />
        <attr name="android:drawableTop" />
        <attr name="android:drawableBottom" />
        <attr name="android:paddingTop" />
        <attr name="android:paddingBottom" />
        <attr name="android:paddingStart" />
        <attr name="android:paddingEnd" />
        <attr name="android:paddingVertical" />
        <attr name="android:paddingHorizontal" />

        <attr name="inputType">
            <enum name="regexp" value="0" />
            <enum name="numeric" value="1" />
            <enum name="alpha" value="2" />
            <enum name="alphaNumeric" value="3" />
            <enum name="email" value="4" />
            <enum name="creditCard" value="5" />
            <enum name="phone" value="6" />
            <enum name="domainName" value="7" />
            <enum name="ipAddress" value="8" />
            <enum name="webUrl" value="9" />
            <enum name="nocheck" value="10" />
            <enum name="custom" value="11" />
            <enum name="personName" value="12" />
            <enum name="personFullName" value="13" />
            <enum name="date" value="14" />
            <enum name="numericRange" value="15" />
            <enum name="floatNumericRange" value="16" />
            <enum name="textEmpty" value="17" />
            <enum name="contains" value="18" />
            <enum name="pass" value="19" />
            <enum name="money" value="20" />
        </attr>

        <attr name="unit_number" format="string" />
        <attr name="custom_pattern" format="string" />
        <attr name="customRegexp" format="string" />
        <attr name="customFormat" format="string" />
        <attr name="emptyAllowed" format="boolean" />
        <attr name="classType" format="string" />
        <attr name="minNumber" format="integer" />
        <attr name="maxNumber" format="integer" />
        <attr name="floatminNumber" format="float" />
        <attr name="floatmaxNumber" format="float" />

        <attr name="errorBackground" format="reference|color" />
        <attr name="errorPaddingStart" format="dimension" />
        <attr name="errorPaddingEnd" format="dimension" />
        <attr name="errorPaddingHorizontal" format="dimension" />
        <attr name="errorPaddingTop" format="dimension" />
        <attr name="errorText" format="string" />
        <attr name="errorTextColor" format="reference|color" />
        <attr name="errorTextSize" format="dimension" />
        <attr name="errorTextStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="moneyFormatCharacter">
            <flag name="dots" value="0" />
            <flag name="comma" value="1" />
        </attr>

    </declare-styleable>

</resources>
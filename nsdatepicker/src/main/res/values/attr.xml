<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="NsDateView">
        <attr name="dateForMat" format="string" />
        <attr name="hourForMat" format="string" />
        <attr name="typeForMat" format="integer">
            <enum name="date" value="0" />
            <enum name="hour" value="1" />
            <enum name="monthOfYear" value="2" />
            <enum name="month" value="3" />
            <enum name="year" value="4" />
        </attr>
        <attr name="defaultDate" format="boolean" />
        // ngăn không cho xem ngày quá khứ
        <attr name="isPast" format="boolean" />
        <attr name="start7Day" format="boolean" />
    </declare-styleable>



</resources>